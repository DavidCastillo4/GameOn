@using GameOn.Web.ViewModel
@model BrowseViewModel

@{
    ViewBag.Title = "Home";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Browse</h1>
<ul>
    @foreach (var product in Model.Products)
    {
        <li>@Html.ActionLink(product.Name, "ProductDetails", new{productId=product.Id})</li>
    }
</ul>