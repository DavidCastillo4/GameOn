<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <script language="javascript" type="text/javascript" src="~/Scripts/vendor/jquery/jquery-3.3.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/js-cookie@2/src/js.cookie.min.js"></script>
    <script language="javascript" type="text/javascript" src="~/Scripts/vendor/bootstrap/bootstrap.bundle.js"></script>
    <link rel="stylesheet" href="~/Content/vendor/bootstrap.min.css" />
    <script language="javascript" type="text/javascript" src="~/Scripts/site.js"></script>
    <link rel="stylesheet" href="~/Content/site.css" />
</head>
<body>


<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            @{ Html.RenderPartial("Navigation"); }
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            @RenderBody()
        </div>
    </div>
</div>
</body>
</html>